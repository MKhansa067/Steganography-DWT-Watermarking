{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "runtime": "python@3.12.0"
    }
  },
  "routes": [
    {
      "src": "/embed",
      "dest": "api/embed.py"
    },
    {
      "src": "/extract",
      "dest": "api/extract.py"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.12.0"
    }
  },
  "installCommand": "pip install -r requirements.txt",
  "ignoreBuildCommand": "true"
}
